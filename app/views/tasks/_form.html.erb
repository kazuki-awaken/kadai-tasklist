<%= form_with(model: task, local: true) do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>
  
  
  <div class="form-group">
    <%= f.label :content, 'タスク'%>
    <%= f.text_field :content %>
    </div>
　<div class="form-group">
    <%= f.label :status, 'ステータス' %>
    <%= f.text_field :status %>
　</div>

  <%= f.submit '保存' %>
<% end %>


